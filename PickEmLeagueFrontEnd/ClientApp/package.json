    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/pickers": "^3.3.10",
    "@mui/lab": "^5.0.0-alpha.45",
    "@mui/material": "^5.0.0-rc.0",
    "@openapitools/openapi-generator-cli": "^2.4.1",
    "@types/react-bootstrap-date-picker": "^4.0.7",
    "@types/react-datepicker": "^4.1.5",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "bootstrap": "^5.1.0",
    "create-react-class": "^15.7.0",
    "dayjs": "^1.10.6",
    "jquery": "^3.6.0",
    "jsx-runtime": "^1.2.0",
    "merge": "^1.2.1",
    "momen": "0.0.1-security",
    "moment": "^2.29.1",
    "oidc-client": "^1.11.5",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-bootstrap-date-picker": "^5.1.0",
    "react-datepicker": "^4.2.1",
    "react-datetime": "^3.0.4",
    "react-datetime-picker": "^3.4.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^7.14.2",
    "react-native": "^0.65.1",
    "react-native-custom-picker": "^0.4.0",
    "react-native-web": "^0.17.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.1",
    "react-scripts": "^3.4.4",
    "react-time-picker": "^4.4.1",
    "reactstrap": "^8.9.0",
    "rimraf": "^2.6.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/react-native": "^0.64.13",
    "@types/react-time-picker": "^4.0.2",
    "ajv": "^6.12.6",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "nan": "^2.15.0",
    "typescript": "^3.9.10"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "build": "react-scripts build",
    "s3": "s3cmd sync build/* s3://2021pickemleague-react",
    "build-sync": "npm run build && npm run s3",
    "api": "npm run pregenerate && npm run generate && npm run postgenerate",
    "pregenerate": "curl https://localhost:5001/swagger/v1/swagger.json > openapi.json",
    "generate": "rimraf ./src/Apisnpm && openapi-generator-cli generate -i openapi.json -o ./src/Apis --generator-name typescript-fetch --additional-properties=typescriptThreePlus=true,supportsES6=true --type-mappings LocalDate=LocalDate",
    "postgenerate": "yarn lint:fix && yarn format:fix",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --ignore-unknown '**/*.*'",
    "format:fix": "yarn format --write",
    "format:check": "yarn format --check"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

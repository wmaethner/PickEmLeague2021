{
  "name": "pickemleague",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@openapitools/openapi-generator-cli": "^2.2.6",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "bootstrap": "^5.1.0",
    "jquery": "^3.5.1",
    "jsx-runtime": "^1.2.0",
    "merge": "^1.2.1",
    "oidc-client": "^1.9.0",
    "prettier": "^2.3.2",
    "react": "^16.14.0",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-dom": "^16.14.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.4",
    "reactstrap": "^8.4.1",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "ajv": "^6.9.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "nan": "^2.14.1",
    "typescript": "^3.9.10"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "build": "react-scripts build",
    "s3": "s3cmd sync build/* s3://2021pickemleague-react",
    "build-sync": "npm run build && npm run s3",
    "pregenerate": "curl https://localhost:5001/swagger/v1/swagger.json > openapi.json",
    "generate": "rimraf ./src/Apisnpm && openapi-generator-cli generate -i openapi.json -o ./src/Apis --generator-name typescript-fetch --additional-properties=typescriptThreePlus=true,supportsES6=true --type-mappings LocalDate=LocalDate",
    "postgenerate": "yarn lint:fix && yarn format:fix",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --ignore-unknown '**/*.*'",
    "format:fix": "yarn format --write",
    "format:check": "yarn format --check"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
